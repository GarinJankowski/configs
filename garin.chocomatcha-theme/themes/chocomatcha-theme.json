  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#878399",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#7caa80"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#cbb871"
      }
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#D1D6BC"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#ad7d49"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#558b8b"
      }
    },
    {
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#7fa294"
      }
    },
    {
      "scope": ["meta.function-call", "meta.method-call"],
      "settings": {
        "foreground": "#558b8b"
      }
    },
    {
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#202721",
        "background": "#ad7d49"
      }
    }
  ],
  "semanticTokenColors": {
    "bracket": {
      "foreground": "#7caa80"
    }
  }
}

